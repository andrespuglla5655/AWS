{
  "family": "aws-demo-task",
  "executionRoleArn": "arn:aws:iam::675082085204:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::675082085204:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "aws-demo",
      "image": "675082085204.dkr.ecr.us-east-2.amazonaws.com/aws-demo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
